/4.Vowel Count/

function getCount(str) {
  var vowelsCount = 0;
  var regex = /[aeiouAEIOU]/;
  
  for (var i = 0; i < str.length; i++) {
    if (regex.test(str[i])) {
    vowelsCount++;
    }
  }
 
  return vowelsCount;
}
getCount("abracadabra");

